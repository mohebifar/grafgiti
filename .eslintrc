{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-space-before-closing": 0,
    "react/jsx-no-bind": 0,
    "no-case-declarations": 0,
    "react/no-unknown-property": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "./src"
        ]
      }
    }
  }
}